	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#92837491"
			}
		},
        {
            "name": "Bloody Red",
            "scope": [
                "variable.other.global",
                "constant.language",
            ],
            "settings": {
                "foreground": "#fb4934"
            }
        },
		{
            "name": "Rose Red",
            "scope": [
                "string",
                "keyword.operator",
                "meta.preprocessor",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "#ff2957"
            }
        },
        {
            "name": "Function",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#ff5277" 
            }
        },
        // {
        //     "name": "Variable Language",
        //     "scope": [
        //         "variable.language"
        //     ],
        //     "settings": {
		// 		"fontStyle": "italic",
		// 		"foreground": "#d3869b"
        //     }
        // },
        {
            "name": "Orange",
            "scope": [
                "constant.numeric",
                "variable.parameter",
            ],
            "settings": {
                "foreground": "#ed812e"
            }
        },
		{
            "name": "Dark Orange",
            "scope": [
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#ac6027"
            }
        },
        {
            "name": "White",
            "scope": [
                "keyword",
                "storage.modifier",
                "meta.block",
                "meta.function",
                "entity.name.type.template",
                "entity.name.namespace",
                "punctuation.separator.namespace",
                "keyword.control.directive"
            ],
            "settings": {
                "foreground": "#e8e7dc"
            }
        },
        {
            "name": "Light Grey",
            "scope": [
                "storage.type",
                "storage.type.template.argument.class",
                "meta.template.definition.cpp",
            ],
            "settings": {
                "foreground": "#a9aaab"
            }
        },
        {
            "name": "Dark Grey",
            "scope": [
                "storage.type.built-in.primitive.cpp",
                "storage.type.template.argument",
                "storage.type.primitive",
                "entity.name.type",
                "source.cmake",
            ],
            "settings": {
                "foreground": "#767f87"
            }
        },
        {
            "name": "Bony",
            "scope": [
                "variable",
                "variable.other.enummember",
            ],
            "settings": {
                "foreground": "#fbf1c7"
            }
        },

        {
            "name": "Markup - Heading",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#F99157"
            }
        },
        {
            "name": "Markup - Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#89B3E7"
            }
        },
        {
            "name": "Markup - List",
            "scope": [
                "markup.list"
            ],
            "settings": {
                "foreground": "#879FD1"
            }
        },
        {
            "name": "Markup - Underline",
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#5e7adf"
            }
        },
        {
            "name": "Markup - Link Title",
            "scope": [
                "string.other.link.title"
            ],
            "settings": {
                "foreground":"#92a9fd"
            }
        },
        {
            "name": "HTML Text, ENV String",
            "scope": [
                "text.html",
                "text.plain",
                "source.env",
                "meta.tag meta.jsx.children"
            ],
            "settings": {
                "foreground": "#AFBAD4"
            }
        },

        {
            "name": "JSON key level 1",
            "scope":  "meta.structure.dictionary.json support.type.property-name.json",
            "settings": {
                "foreground": "#ed812e"
            }
        },
        {
            "name": "JSON key level 2",
            "scope": "meta meta.structure.dictionary.value.json support.type.property-name.json",
            "settings": {
                "foreground": "#fbf1c7"
            }
        },
	]
