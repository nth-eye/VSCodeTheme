    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment"
            ],
            "settings": {
                "fontStyle": "italic",
        		"foreground": "#51546a"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#ca9b55"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
